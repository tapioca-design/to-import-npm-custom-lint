{
  "name": "to-import-npm-custom-lint",
  "type": "module",
  "description": "to-import-npm-custom-lint",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": "https://dev.azure.com/yooag/Hitachi/_git/collapp/"
  },
  "private": true,
