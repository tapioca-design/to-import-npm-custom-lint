  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve --configuration=development",
    "start-pwa": "http-server -a localhost -p 8080 -P http://localhost:8080? -c-1 dist/collapp",
    "build": "npm run build:prod",
    "build:local": "cross-env BUILD_BUILDNUMBER=DEV ng build --configuration=integration-local",
    "build:dev": "ng build --configuration=integration",
    "build:stage": "ng build --configuration=staging",
    "build:prod": "ng build --configuration=production",
    "build:stats": "ng build --configuration=production --stats-json",
    "build-report": "npm run build:stats && npm run analyze",
    "test": "ng test --watch=false",
    "test:coverage": "ng test --coverage",
    "test:watch": "ng test --watch",
    "lintci": "ng lint",
    "stylelint": "stylelint",
    "lint": "npm run lint:ts && npm run lint:sass",
    "lint:sass": "npm run stylelint \"src/**/*.scss\" ",
    "lint:ts": "eslint \"src/app/**/*.{ts,html}\" ",
    "lint:ts-errors-only": "eslint \"src/app/**/*.{ts,html}\" --quiet",
    "lint:fix": "eslint \"src/app/**/*.{ts,html}\" --quiet --fix",
    "lint:ci": "npm run lint && npm run stylelint",
    "autofix": "npm run autofix:sass && npm run autofix:ts",
    "autofix:sass": "npm run lint:sass --fix",
    "autofix:ts": "npm run lint:ts --fix",
    "analyze": "npm run analyze:es5 & npm run analyze:es2015",
    "ci": "run-s lint:ci build:prod",
    "format": "prettier . --config .prettierrc -c --write",
    "format:check": "prettier . --config .prettierrc -l",
    "lint:ls": "ls-lint"
  },
